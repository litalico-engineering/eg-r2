includes:
  - ./vendor/spaze/phpstan-disallowed-calls/extension.neon
  - phpstan-baseline.neon
parameters:
  level: 9
  paths:
    - ./src/
  excludePaths:
    - tests/**/*.php
  tmpDir: .phpstan
  reportUnmatchedIgnoredErrors: false
  disallowedFunctionCalls:
    -
      function: 'collect()'
      message: 'use new Collection instead'
